{
  "name": "service",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/team-iroh/za-service#readme",
  "bugs": {
    "url": "https://github.com/team-iroh/za-service/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-iroh/pledge-rewards.git"
  },
  "license": "ISC",
  "author": "Zach Ayers",
  "main": "server/src/index.js",
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "dev": "concurrently 'npm run dev:server' 'npm run dev:client'",
    "dev:client": "cd client && npm run build-dev:watch",
    "dev:server": "cd server && npm run dev",
    "format": "(cd server && npm run format); (cd client && npm run format);",
    "postinstall": "(cd client && npm install); (cd server && npm install);",
    "lint": "(cd server && npm run lint); (cd client && npm run lint);",
    "precommit-msg": "echo 'Project parsing...' && exit 0",
    "sort": "(npx sort-package-json); (cd server && npm run sort); (cd client && npm run sort);",
    "prestart": "cd client && npm run build-prod",
    "start": "cd server && npm run start",
    "start:database": "node server/src/dev-data/seeds/projectSeed.js --docker && npm run start",
    "test": "(cd server && npm run test); (cd client && npm run test);"
  },
  "pre-commit": [
    "sort",
    "lint",
    "format"
  ],
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^5.2.0",
    "gulp": "^4.0.2",
    "pre-commit": "^1.2.2"
  }
}
